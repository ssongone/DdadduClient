<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DdadduBot.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DdadduBot"
    Shell.FlyoutBehavior="Disabled"
    Title="DdadduBot">

    <TabBar>
        <Tab Title="Home" >
            <ShellContent ContentTemplate="{DataTemplate local:Hi}" />
        </Tab>
        <Tab Title="Settings">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
        </Tab>
    </TabBar>
    
    <ShellContent
        Title="Hi"
        ContentTemplate="{DataTemplate local:Hi}"
        Route="Hi" />
    
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />


</Shell>
